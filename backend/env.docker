APP_NAME=replay-platform
APP_ENV=development
APP_DEBUG=true
APP_VERSION=1.0.0

API_V1_PREFIX=/api/v1

# Docker Compose 服务下使用容器网络
DATABASE_URL=postgresql://replay_user:replay_password@postgres:5432/replay_db
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/1

# CORS
CORS_ALLOW_ORIGINS=["http://localhost:3000","http://localhost:5173","http://localhost:5174"]
CORS_ALLOW_CREDENTIALS=true


